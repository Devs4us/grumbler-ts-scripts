{
  "name": "grumbler-ts-scripts",
  "version": "1.1.0",
  "main": "index.js",
  "description": "Typescript shared scripting template",
  "license": "MIT",
  "scripts": {
    "watch": "webpack --watch",
    "setup": "yarn",
    "build": "yarn types:build && yarn babel && yarn bundle",
    "start": "webpack-dev-server --progress --colors --open",
    "clean": "rimraf dist coverage",
    "babel": "babel ./src --ignore=node_modules --extensions=\".ts,.tsx\" --out-dir ./config",
    "bundle": "webpack --progress --colors",
    "types:build": "tsc --emitDeclarationOnly",
    "types:check": "tsc --noEmit",
    "types:watch": "yarn types:check -- --watch",
    "check-updates": "npm-check-updates",
    "watch:p": "parallel-webpack --config=webpack.config.babel.js --watch",
    "build:p": "yarn types:build && yarn babel && yarn bundle:p",
    "bundle:p": "parallel-webpack --config=webpack.config.babel.js --progress --colors"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-scoped-css": "^1.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "cache-loader": "^4.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "css-loader": "^4.2.2",
    "file-loader": "^6.1.0",
    "fs-extra": "^9.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "isomorphic-style-loader": "^5.1.0",
    "node-cleanup": "^2.1.2",
    "npm-check-updates": "^8.1.0",
    "parallel-webpack": "^2.6.0",
    "postcss-loader": "^3.0.0",
    "process-exists": "^4.0.0",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "rmfr": "^2.0.0",
    "sass-loader": "^10.0.2",
    "scoped-css-loader": "^1.0.0",
    "semver": "^7.3.2",
    "terser-webpack-plugin": "^4.1.0",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "utility-types": "^3.10.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@types/circular-dependency-plugin": "^5.0.1",
    "@types/fs-extra": "^9.0.1",
    "@types/hard-source-webpack-plugin": "^1.0.1",
    "@types/node-cleanup": "^2.1.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/rimraf": "^3.0.0",
    "@types/rmfr": "^2.0.0",
    "@types/semver": "^7.3.3",
    "@types/terser-webpack-plugin": "^4.1.0",
    "@types/webpack": "^4.41.22",
    "@types/webpack-bundle-analyzer": "^3.8.0"
  },
  "browserslist": [
    "IE >= 9",
    "chrome >= 27",
    "firefox >= 30",
    "safari >= 5",
    "opera >= 23"
  ],
  "files": [
    "config",
    "declarations.ts",
    "src",
    "@types"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Devs4us/grumbler-ts-scripts.git"
  },
  "keywords": [
    "typescript template",
    "typescript boilerplate"
  ],
  "licenses": [
    {
      "type": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],
  "readmeFilename": "README.md"
}
